{
    "name": "webpack-updated",
    "version": "1.0.7",
    "description": "Focus webpack config",
    "main": "./index.js",
    "bin": {
        "focus-dev-server": "./launcher.js"
    },
    "scripts": {
        "build": "better-npm-run build",
        "release": "npm publish --registry https://registry.npmjs.org/ --access public",
        "release-beta": "npm publish --registry https://registry.npmjs.org/ --tag beta --access public",
        "release-alpha": "npm publish --registry http://localhost:4873 --tag alpha --access public"
    },
    "babel": {
        "presets": [
            "updated"
        ]
    },
    "betterScripts": {
        "build": {
            "command": "babel src/ --out-dir .",
            "env": {
                "NODE_ENV": "development",
                "LEGACY_EXPORTS": "true"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anasamo92/webpack-updated.git"
    },
    "keywords": [
        "focus",
        "webpack"
    ],
    "license": "MIT",
    "devDependencies": {
        "tslint": "^6.1.3",
        "typescript": "^4.1.5",
        "@babel/cli": "^7.12.16",
        "better-npm-run": "^0.1.1",
        "lodash": "^4.17.20"
    },
    "dependencies": {
        "autoprefixer": "^10.2.4",
        "babel-loader": "^8.2.2",
        "babel-preset-updated": "^1.0.2",
        "cache-loader": "^4.1.0",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "css-loader": "^5.0.2",
        "cssnano": "^4.1.10",
        "eslint-config-updated": "^1.0.2",
        "eslint-loader": "^4.0.2",
        "extract-text-webpack-plugin-updated": "^1.0.3",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "html-webpack-plugin": "^5.1.0",
        "mini-css-extract-plugin": "^1.3.6",
        "node-sass": "^5.0.0",
        "postcss-color-function": "^4.1.0",
        "postcss-custom-properties": "^11.0.0",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-import": "^14.0.0",
        "postcss-loader": "^5.0.0",
        "postcss-mixins": "^7.0.3",
        "postcss-normalize": "^9.0.0",
        "postcss-preset-env": "^6.7.0",
        "react-dev-utils": "^11.0.2",
        "react-hot-loader": "^4.13.0",
        "sass-loader": "^11.0.1",
        "source-map-loader": "^2.0.1",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^5.1.1",
        "thread-loader": "^3.0.1",
        "ts-loader": "^8.0.17",
        "url-loader": "^4.1.1",
        "webpack": "^5.21.2",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "whatwg-fetch": "^3.5.0"
    }
}
