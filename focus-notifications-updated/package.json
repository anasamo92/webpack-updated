{
    "name": "focus-notifications-updated",
    "version": "1.0.6",
    "description": "Notification Center extension for focus",
    "main": "index.js",
    "scripts": {
        "babelify": "better-npm-run babelify",
        "build": "better-npm-run babelify && better-npm-run copy-sass",
        "bundle": "better-npm-run bundle",
        "copy-sass": "better-npm-run copy-sass",
        "lint": "eslint src",
        "start": "better-npm-run dev-server",
        "test": "echo \"No test specified\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anasamo92/focus-notifications-updated.git"
    },
    "betterScripts": {
        "copy-sass": {
            "command": "cd src && find . -name *.scss | xargs tar cvf - | tar xfp - -C ../"
        },
        "babelify": {
            "env": {
                "BABEL_ENV": "production"
            },
            "command": "node script/babelify.js"
        },
        "test": {
            "command": "node ./node_modules/mocha/bin/mocha src/**/__tests__/**/*.js"
        }
    },
    "babel": {
        "presets": [
            "updated"
        ]
    },
    "keywords": [
        "notification",
        "focus"
    ],
    "license": "MIT",
    "files": [
        "__tests__",
        "actions",
        "api-mock",
        "component",
        "config.js",
        "container",
        "dist",
        "example",
        "index.html",
        "index.js",
        "LICENSE",
        "package.json",
        "README.md",
        "reducers",
        "src",
        "server.js",
        "store",
        "style",
        "test",
        "translation",
        "util",
        "webpack.config.js"
    ],
    "dependencies": {
        "isomorphic-fetch": "^3.0.0",
        "redux-devtools": "^3.7.0",
        "redux-devtools-dock-monitor": "^1.2.0",
        "redux-devtools-log-monitor": "^2.1.0"
    },
    "peerDependencies": {
        "lodash": "^4.17.20",
        "material-design-lite": "^1.3.0",
        "material-design-icons-iconfont": "^6.1.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "babel-preset-updated": "^1.0.2",
        "better-npm-run": "^0.1.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-runtime": "^6.26.0",
        "chai": "^4.3.0",
        "chai-subset": "^1.6.0",
        "eslint": "^7.19.0",
        "eslint-config-updated": "^1.0.2",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-react": "^7.22.0",
        "express": "^4.17.1",
        "faker": "^5.3.1",
        "i18next": "^19.8.7",
        "jsdom": "^16.4.0",
        "lodash": "^4.17.20",
        "material-design-icons-iconfont": "^6.1.0",
        "material-design-lite": "^1.3.0",
        "mocha": "^8.2.1",
        "moment": "^2.29.1",
        "nock": "^13.0.7",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-addons-test-utils": "^15.6.2",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "redux-logger": "^3.0.6",
        "sinon": "^9.2.4",
        "sinon-chai": "^3.5.0"
    }
}
